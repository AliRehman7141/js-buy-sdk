{
  "name": "shopify-buy",
  "version": "0.2.1",
  "description": "The JS Buy SDK is a lightweight library that allows you to build ecommerce into any website. It is based on Shopify's API and provides the ability to retrieve products and collections from your shop, add products to a cart, and checkout.",
  "main": "lib/shopify.js",
  "repository": "git@github.com:Shopify/js-buy-sdk.git",
  "scripts": {
    "build": "node ./scripts/build",
    "prepublish": "node ./scripts/prepublish",
    "doc-server": "yuidoc --server 5000",
    "start": "broccoli serve",
    "examples": "env EXAMPLES=1 broccoli serve",
    "test": "node ./scripts/test",
    "test-examples": "node scripts/test-examples",
    "test-ci": "./scripts/ci",
    "------ New NPM script based builds start here": null,
    "test2": "npm run build-test; testem ci",
    "test-ci2": "npm run build-test; testem ci -R xunit > tests.xml",
    "build-all": "npm run dir-setup-dist; npm run amd; npm run umd; npm run umd-polyfilled; npm run commonjs",
    "------ Scripts used to build JS for distribution": null,
    "build-production": "npm run build-all; npm run minify-all",
    "umd": "npm run build-umd; npm run header-umd",
    "umd-polyfilled": "npm run build-umd-polyfilled; npm run header-umd-polyfilled",
    "commonjs": "cross-env BABEL_ENV=commonjs babel -d lib/ src/; node scripts/addVersion lib/version.js",
    "build-umd": "rollup -f cjs src/shopify.js | node scripts/runBabel umd > dist/shopify-buy.umd.js",
    "header-umd": "node scripts/addHeaderToSource.js dist/shopify-buy.umd.js",
    "build-umd-polyfilled": "rollup --config .rollup.config.umd.polfilled.js | node scripts/runBabel umd > dist/shopify-buy.umd.polyfilled.js",
    "header-umd-polyfilled": "node scripts/addHeaderToSource.js dist/shopify-buy.umd.polyfilled.js",
    "------ Scripts to compile JS for testing": null,
    "build-test": "npm run rm-dist-test; npm run dir-setup-dist; npm run amd; broccoli build .dist-test; cp dist/shopify-buy.amd.js .dist-test/shopify-buy.amd.js",
    "amd": "npm run build-amd; npm run header-amd",
    "build-amd": "cross-env BABEL_ENV=amd babel src/ > dist/shopify-buy.amd.js src/",
    "header-amd": "node scripts/addHeaderToSource.js dist/shopify-buy.amd.js",
    "------ Scripts for testing-examples": null,
    "###### below build-testing-sdk.js will concat a few files and set `window.fetch=null` for testing purposes": null,
    "build-test-sdk": "node scripts/build-testing-sdk.js > .dist-test/sdk-testing.js; npm run copy-qunit",
    "copy-qunit": "cp node_modules/qunitjs/qunit/qunit.js .dist-test/qunit.js; cp node_modules/qunitjs/qunit/qunit.css .dist-test/qunit.css",
    "------ Scripts to minify code": null,
    "minify-all": "npm run minify-amd; npm run minify-umd; npm run minify-umd-polyfilled",
    "minify-amd": "uglifyjs --compress --mangle -o dist/shopify-buy.amd.min.js -- dist/shopify-buy.amd.js",
    "minify-umd": "uglifyjs --compress --mangle -o dist/shopify-buy.umd.min.js -- dist/shopify-buy.umd.js",
    "minify-umd-polyfilled": "uglifyjs --compress --mangle -o dist/shopify-buy.umd.polyfilled.min.js -- dist/shopify-buy.umd.polyfilled.js",
    "------ The following are all file system operations used to make, and copy existing files": null,
    "dir-setup-all": "npm run dir-setup-dist; npm run dir-setup-dist-test;",
    "dir-setup-dist": "npm run rm-dist; npm run mk-dist;",
    "dir-setup-dist-test": "npm run rm-dist-test; npm run mk-dist-test;",
    "mk-dist": "mkdir dist/",
    "rm-dist": "rimraf dist/",
    "mk-dist-test": "mkdir .dist-test/",
    "rm-dist-test": "rimraf .dist-test/"
  },
  "author": "Shopify Inc.",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "1.3.3"
  },
  "devDependencies": {
    "Base64": "0.3.0",
    "aws-sdk": "2.2.44",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "5.0.0",
    "babel-plugin-rename-umd-globals": "0.0.3",
    "babel-plugin-transform-es2015-modules-amd": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.8.0",
    "babel-plugin-transform-es2015-modules-umd": "6.12.0",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "6.9.0",
    "broccoli": "0.16.9",
    "broccoli-asset-rev": "2.4.1",
    "broccoli-babel-transpiler": "git+ssh://git@github.com/babel/broccoli-babel-transpiler.git#17b304da9a1c2c93ff484e3d37029f5fad0f96c9",
    "broccoli-cli": "1.0.0",
    "broccoli-concat": "2.2.0",
    "broccoli-env": "0.0.1",
    "broccoli-funnel": "1.0.1",
    "broccoli-lint-eslint": "2.4.1",
    "broccoli-merge-trees": "1.1.1",
    "broccoli-plugin": "1.2.1",
    "broccoli-sane-watcher": "1.1.4",
    "broccoli-sass-source-maps": "1.7.0",
    "broccoli-static-compiler": "0.2.2",
    "broccoli-uglify-js": "0.1.3",
    "broccoli-uglify-sourcemap": "1.1.1",
    "escope": "3.3.0",
    "eslint-config-nightmare-mode": "2.3.0",
    "estraverse-fb": "1.3.1",
    "fake-xml-http-request": "1.4.0",
    "fs-extra": "0.30.0",
    "get-stdin": "5.0.1",
    "git-rev": "0.2.1",
    "global-npm": "0.3.0",
    "js-string-escape": "1.0.0",
    "loader.js": "3.5.0",
    "mime-types": "2.1.10",
    "mocha": "2.5.3",
    "nodegit": "0.13.0",
    "pretender": "0.10.1",
    "qunitjs": "1.20.0",
    "rollup": "0.34.7",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "route-recognizer": "0.1.9",
    "rsvp": "3.1.0",
    "testem": "0.9.11",
    "uglify-js": "^2.7.0",
    "wdio-dot-reporter": "0.0.5",
    "wdio-junit-reporter": "0.1.0",
    "wdio-mocha-framework": "^0.3.1",
    "webdriverio": "4.0.9",
    "whatwg-fetch": "0.10.1",
    "yuidocjs": "0.10.0"
  }
}
