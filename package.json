{
  "name": "shopify-buy",
  "version": "0.2.1",
  "description": "The JS Buy SDK is a lightweight library that allows you to build ecommerce into any website. It is based on Shopify's API and provides the ability to retrieve products and collections from your shop, add products to a cart, and checkout.",
  "main": "lib/shopify.js",
  "repository": "git@github.com:Shopify/js-buy-sdk.git",
  "scripts": {
    "build": "node ./scripts/build",
    "prepublish": "node ./scripts/prepublish",
    "doc-server": "yuidoc --server 5000",
    "start": "broccoli serve",
    "examples": "env EXAMPLES=1 broccoli serve",
    "test": "node ./scripts/test",
    "test-examples": "node scripts/test-examples",
    "test-ci": "./scripts/ci",

    "test-babel": "npm run test-build-babel; testem ci",
    "test-build-babel": "npm run babel-build; cp dist/shopify-buy.amd.js .dist-test/shopify-buy.amd.js",

    "babel-build": "npm run rm-dist; npm run mk-dist; npm run babel-amd; npm run babel-umd; npm run babel-cjs",
    "babel-amd": "npm run babel-source-amd; npm run header-amd;",
    "babel-umd": "npm run babel-source-umd; npm run header-umd;",
    "babel-cjs": "npm run babel-source-cjs; npm run header-cjs;",
    "header-amd": "node scripts/addHeaderToSource.js dist/shopify-buy.amd.js",
    "header-umd": "node scripts/addHeaderToSource.js dist/shopify-buy.umd.js",
    "header-cjs": "node scripts/addHeaderToSource.js dist/shopify-buy.commonjs.js",
    "babel-source-amd": "babel --source-root src/ --presets es2015 --plugins babel-plugin-transform-es2015-modules-amd --module-root shopify-buy --module-ids -o dist/shopify-buy.amd.js src/",
    "babel-source-umd": "babel --source-root src/ --presets es2015 --plugins babel-plugin-transform-es2015-modules-umd --module-root shopify-buy --module-ids -o dist/shopify-buy.umd.js src/",
    "babel-source-cjs": "babel --source-root src/ --presets es2015 --plugins babel-plugin-transform-es2015-modules-commonjs --module-root shopify-buy --module-ids -o dist/shopify-buy.commonjs.js src/",
    "mk-dist": "mkdir dist/",
    "rm-dist": "rm -rf dist/"
  },
  "author": "Shopify Inc.",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "1.3.3"
  },
  "devDependencies": {
    "Base64": "0.3.0",
    "aws-sdk": "2.2.44",
    "babel-cli": "^6.11.4",
    "babel-eslint": "5.0.0",
    "babel-plugin-transform-es2015-modules-amd": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.8.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "6.9.0",
    "broccoli": "0.16.9",
    "broccoli-asset-rev": "2.4.1",
    "broccoli-babel-transpiler": "git+ssh://git@github.com/babel/broccoli-babel-transpiler.git#17b304da9a1c2c93ff484e3d37029f5fad0f96c9",
    "broccoli-cli": "1.0.0",
    "broccoli-concat": "2.2.0",
    "broccoli-env": "0.0.1",
    "broccoli-funnel": "1.0.1",
    "broccoli-lint-eslint": "2.4.1",
    "broccoli-merge-trees": "1.1.1",
    "broccoli-plugin": "1.2.1",
    "broccoli-sane-watcher": "1.1.4",
    "broccoli-sass-source-maps": "1.7.0",
    "broccoli-static-compiler": "0.2.2",
    "broccoli-uglify-js": "0.1.3",
    "broccoli-uglify-sourcemap": "1.1.1",
    "escope": "3.3.0",
    "eslint-config-nightmare-mode": "2.3.0",
    "estraverse-fb": "1.3.1",
    "fake-xml-http-request": "1.4.0",
    "fs-extra": "0.30.0",
    "git-latest-semver-tag": "^1.0.2",
    "git-rev": "^0.2.1",
    "global-npm": "0.3.0",
    "js-string-escape": "1.0.0",
    "loader.js": "3.5.0",
    "mime-types": "2.1.10",
    "mocha": "2.5.3",
    "nodegit": "0.13.0",
    "pretender": "0.10.1",
    "qunitjs": "1.20.0",
    "route-recognizer": "0.1.9",
    "rsvp": "3.1.0",
    "testem": "0.9.11",
    "wdio-dot-reporter": "0.0.5",
    "wdio-junit-reporter": "0.1.0",
    "wdio-mocha-framework": "^0.3.1",
    "webdriverio": "4.0.9",
    "whatwg-fetch": "0.10.1",
    "yuidocjs": "0.10.0"
  }
}
